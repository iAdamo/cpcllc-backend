{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@config/*": ["src/config/*"],
      "@types": ["src/types/index.d.ts"],
      "@*": ["src/modules/*"],
      "@modules/*": [
        "src/modules/users/*",
        "src/modules/auth/*",
        "src/modules/services/*",
        "src/modules/review/*",
        "src/modules/provider/*",
        "src/modules/media/*",
        "src/modules/search/*",
        "src/modules/admin/*",
        "src/modules/cache/*",
        "src/modules/websocket/*",
        "src/modules/chat/*"

      ],
      "@schemas/*": [
        "src/modules/users/schemas/*",
        "src/modules/services/schemas/*",
        "src/modules/review/schemas/*",
        "src/modules/provider/schemas/*",
        "src/modules/chat/schemas/*",
        "src/modules/notification/schemas/*"
      ],
      "@dto/*": [
        "src/modules/users/dto/*",
        "src/modules/services/dto/*",
        "src/modules/auth/dto/*",
        "src/modules/review/dto/*",
        "src/modules/admin/dto/*",
        "src/modules/provider/dto/*"
      ],
      "@controllers/*": [
        "src/modules/users/*",
        "src/modules/auth/*",
        "src/modules/services/*",
        "src/modules/search/*",
        "src/modules/review/*",
        "src/modules/provider/*",
        "src/modules/notification/*",
        "src/modules/chat/*"
      ],
      "@services/*": [
        "src/modules/users/*",
        "src/modules/auth/*",
        "src/modules/services/*",
        "src/modules/search/*",
        "src/modules/services",
        "src/modules/review/*",
        "src/modules/provider/*",
        "src/modules/admin/*",
        "src/modules/cache/*",
        "src/modules/notification/*",
        "src/modules/chat/*"
      ],
      "@guards/*": ["src/modules/auth/jwt/*"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": false
  }
}
